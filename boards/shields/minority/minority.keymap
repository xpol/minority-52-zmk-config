// The Minority Keyboard
//

#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/outputs.h>
#include <dt-bindings/zmk/mouse.h>

#define BASE 0
#define SYM  1
#define NUM  2
#define NAV  3
#define FUN  4

&mt {
  quick-tap-ms = <150>;
  flavor = "tap-preferred";
  tapping-term-ms = <200>;
};


&lt {
  quick-tap-ms = <150>;
  flavor = "tap-preferred";
  tapping-term-ms = <200>;
};

/ {
    combos {
        compatible = "zmk,combos";
        combo_esc {
            timeout-ms = <50>;
            key-positions = <0 1>;
            bindings = <&kp ESC>;
        };
    };

    keymap {
        compatible = "zmk,keymap";

        base_layer {
//////////////////////////////////////////////
            display-name = "Base";
//////////////////////////////////////////////
            bindings = <
// +-------------+-------------+-------------+-------------+-------------+-------------+                               +-------------+-------------+-------------+-------------+-------------+-------------+
    &kp EQUAL     &kp N1        &kp N2        &kp N3        &kp N4        &kp N5                                        &kp N6        &kp N7        &kp N8        &kp N9        &kp N0        &kp MINUS
// +-------------+-------------+-------------+-------------+-------------+-------------+                               +-------------+-------------+-------------+-------------+-------------+-------------+
    &kp CAPS      &kp GRAVE     &kp W         &kp F         &kp P         &kp B                                         &kp J         &kp L         &kp U         &kp Y         &kp FSLH      &kp BSLH
// +-------------+-------------+-------------+-------------+-------------+-------------+                               +-------------+-------------+-------------+-------------+-------------+-------------+
    &kp Q         &mt LCTRL A   &mt LALT R    &mt LGUI S    &mt LSHFT T   &kp G                                         &kp M         &mt RSHFT N   &mt RGUI E    &mt RALT I    &mt RCTRL O   &kp APOS
// +-------------+-------------+-------------+-------------+-------------+-------------+                               +-------------+-------------+-------------+-------------+-------------+-------------+
                  &kp Z         &kp X         &kp C         &kp D         &kp V                                         &kp K         &kp H         &kp COMMA     &kp DOT       &kp SEMI
//               +-------------+-------------+-------------+-------------+-------------+-------------+---+-------------+-------------+-------------+-------------+-------------+-------------+
                                                            &mt LGUI ESC  &lt NAV SPC   &lt SYM TAB       &lt SYM RET   &lt NUM BSPC  &lt FUN DEL
//                                                         |     ESC     |     SPC     |     TAB     |   |    ENTER    |    BSPC     |     DEL     |
//                                                         +-------------+-------------+-------------+---+-------------+-------------+-------------+
            >;
        };

        symbol_layer {
//////////////////////////////////////////////
            display-name = "Sym";
//////////////////////////////////////////////
            bindings = <
// +-------------+-------------+-------------+-------------+-------------+-------------+                               +-------------+-------------+-------------+-------------+-------------+-------------+
    &none         &none         &none         &none         &none         &none                                         &none         &none         &none         &none         &none         &none 
// +-------------+-------------+-------------+-------------+-------------+-------------+                               +-------------+-------------+-------------+-------------+-------------+-------------+
    &none         &none         &kp LT        &kp GT        &kp GRAVE     &kp DQT                                       &kp AMPS      &kp AT        &kp LBKT      &kp RBKT      &none         &none 
// +-------------+-------------+-------------+-------------+-------------+-------------+                               +-------------+-------------+-------------+-------------+-------------+-------------+
    &kp CARET     &kp EXCL      &kp MINUS     &kp PLUS      &kp EQUAL     &kp UNDER                                     &kp PIPE      &kp COLON     &kp LPAR      &kp RPAR      &kp QMARK     &kp PERCENT
// +-------------+-------------+-------------+-------------+-------------+-------------+                               +-------------+-------------+-------------+-------------+-------------+-------------+
                  &kp COMMA     &kp FSLH      &kp ASTRK     &kp BSLH      &kp HASH                                      &kp TILDE     &kp DOLLAR    &kp LBRC      &kp RBRC      &kp DOT
//               +-------------+-------------+-------------+-------------+-------------+-------------+---+-------------+-------------+-------------+-------------+-------------+-------------+
                                                            &trans        &trans        &trans            &trans        &trans        &trans
//                                                         |     ESC     |     SPC     |     TAB     |   |    ENTER    |    BSPC     |     DEL     |
//                                                         +-------------+-------------+-------------+---+-------------+-------------+-------------+
            >;
        };

        num_layer {
//////////////////////////////////////////////
            display-name = "Num";
//////////////////////////////////////////////
            bindings = <
// +-------------+-------------+-------------+-------------+-------------+-------------+                               +-------------+-------------+-------------+-------------+-------------+-------------+
    &none         &none         &none         &none         &none         &none                                         &none         &none         &none         &none         &none           &none 
// +-------------+-------------+-------------+-------------+-------------+-------------+                               +-------------+-------------+-------------+-------------+-------------+-------------+
    &none         &none         &none         &none         &none         &kp LPAR                                      &kp KP_MINUS  &kp KP_N7     &kp KP_N8     &kp KP_N9     &none           &none 
// +-------------+-------------+-------------+-------------+-------------+-------------+                               +-------------+-------------+-------------+-------------+-------------+-------------+
    &none         &none         &none         &none         &none         &kp RPAR                                      &kp KP_PLUS   &kp KP_N1     &kp KP_N2     &kp KP_N3     &kp KP_MULTIPLY &kp KP_DIVIDE
// +-------------+-------------+-------------+-------------+-------------+-------------+                               +-------------+-------------+-------------+-------------+-------------+-------------+
                  &none         &none         &none         &none         &kp CLEAR2                                    &kp KP_EQUAL  &kp KP_N4     &kp KP_N5     &kp KP_N6     &kp KP_DOT
//               +-------------+-------------+-------------+-------------+-------------+-------------+---+-------------+-------------+-------------+-------------+-------------+-------------+
                                                            &trans        &trans        &trans            &trans        &trans        &kp KP_N0
//                                                         |     ESC     |     SPC     |     TAB     |   |    ENTER    |    BSPC     |     DEL     |
//                                                         +-------------+-------------+-------------+---+-------------+-------------+-------------+
            >;
        };

        nav_layer {
//////////////////////////////////////////////
            display-name = "Nav";
//////////////////////////////////////////////
            bindings = <
// +-------------+-------------+-------------+-------------+-------------+-------------+                               +-------------+-------------+-------------+-------------+-------------+-------------+
    &none         &none         &none         &none         &none         &none                                         &none         &none         &none         &none         &none         &none 
// +-------------+-------------+-------------+-------------+-------------+-------------+                               +-------------+-------------+-------------+-------------+-------------+-------------+
    &none         &none         &kp LG(X)     &kp LG(C)     &kp LG(V)     &kp LG(Z)                                     &none         &none         &none         &none         &none         &none 
// +-------------+-------------+-------------+-------------+-------------+-------------+                               +-------------+-------------+-------------+-------------+-------------+-------------+
    &kp HOME      &kp LEFT      &kp UP        &kp DOWN      &kp RIGHT     &kp END                                       &none         &none         &none         &none         &none         &none 
// +-------------+-------------+-------------+-------------+-------------+-------------+                               +-------------+-------------+-------------+-------------+-------------+-------------+
                  &kp CAPS      &kp PAGE_UP   &kp PAGE_DOWN &kp INSERT    &kp LG(LS(Z))                                 &none         &none         &none         &none         &none 
//               +-------------+-------------+-------------+-------------+-------------+-------------+---+-------------+-------------+-------------+-------------+-------------+-------------+
                                                            &trans        &trans            &trans        &trans        &trans        &trans
//                                                         |     ESC     |     SPC     |     TAB     |   |    ENTER    |    BSPC     |     DEL     |
//                                                         +-------------+-------------+-------------+---+-------------+=============+-------------+
            >;
        };

        func_layer {
//////////////////////////////////////////////
            display-name= "Fun";
//////////////////////////////////////////////
            bindings = <
// +-------------+-------------+-------------+-------------+-------------+-------------+                               +-------------+-------------+-------------+-------------+-------------+-------------+
    &none         &none         &none         &none         &none         &none                                         &none         &none         &none         &none         &none         &studio_unlock
// +-------------+-------------+-------------+-------------+-------------+-------------+                               +-------------+-------------+-------------+-------------+-------------+-------------+
    &none         &none         &kp F10       &kp F11       &kp F12       &kp C_VOL_UP                                  &bt BT_SEL 0  &bt BT_SEL 1  &bt BT_SEL 2  &bt BT_SEL 3  &none         &none 
// +-------------+-------------+-------------+-------------+-------------+-------------+                               +-------------+-------------+-------------+-------------+-------------+-------------+
    &kp F9        &kp F1        &kp F2        &kp F3        &kp F4        &kp C_VOL_DN                                  &none         &none         &none         &none         &none         &none 
// +-------------+-------------+-------------+-------------+-------------+-------------+                               +-------------+-------------+-------------+-------------+-------------+-------------+
                  &kp F5        &kp F6        &kp F7        &kp F8        &kp C_MUTE                                    &none         &none         &none         &none         &bt BT_CLR
//               +-------------+-------------+-------------+-------------+-------------+-------------+---+-------------+-------------+-------------+-------------+-------------+-------------+
                                                            &trans        &trans        &trans            &trans        &trans        &trans
//                                                         |     ESC     |     SPC     |     TAB     |   |    ENTER    |    BSPC     |     DEL     |
//                                                         +-------------+-------------+-------------+---+-------------+-------------+=============+
            >;
        };
        extra1 {
            status = "reserved";
        };

        extra2 {
            status = "reserved";
        };
    };
};
